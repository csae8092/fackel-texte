            <p ana="pr29399">
<B>ORIGINAL-EINBANDDECKEN</B><lb/>
</p>
            <p ana="pr29400">
<B>der ‚<rs type="self" ana="werk_10751">Fackel</rs>‘</B><lb/>
</p>
            <p ana="pr29401">
(<DATERANGE>October-December 1902</DATERANGE> <seg type="symbol" subtype="equals"/> <rs type="selfRef" ana="werk_10752"><abbr>Nr.</abbr> 118</rs> bis <rs type="selfRef" ana="werk_10753">125</rs>) sammt <B>Index</B><lb/>
